{
  "name": "Squid",
  "version": "1.1.0",
  "description": "A very simple application to create banners",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "test": "xo",
    "pack": "xo && build --dir",
    "dist": "build",
    "dist-win": "build --win",
    "dist-mac": "build --mac",
    "dist-linux": "build --linux"
  },
  "xo": {
    "space": 4,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "import/no-unassigned-import": 0
    }
  },
  "repository": "",
  "keywords": [
    "electron",
    "knockout",
    "redux"
  ],
  "author": "Oscar Rundh",
  "license": "",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.6.15",
    "electron-builder": "^14.4.0",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "knockout": "^3.4.1",
    "node-base64-image": "^1.0.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
